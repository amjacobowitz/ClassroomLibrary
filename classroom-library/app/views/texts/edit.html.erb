<div class="container">
   <%= form_for @text do |f| %>
    <%= f.text_field :isbn %>
    <br>
    <%= f.text_field :title %>
    <br>
    <%= f.text_field :lexile_level %>
    <br>
    <%= f.text_field :pages %>
    <br>
    <%= f.text_field :author%>
    <br>
    <%= f.text_field :genre%>
    <br>
    <%= f.submit 'Add a book' %>
  <% end %>
</div>
